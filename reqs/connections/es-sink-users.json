{
  "name": "elasticsearch-sink-connector",
  "config": {
    "connector.class": "io.confluent.connect.elasticsearch.ElasticsearchSinkConnector",
    "tasks.max": "1",
    "topics": "cdc_moderators",
    "connection.url": "http://elasticsearch:9200",
    "key.ignore": "true",
    "auto.create": "false",
    "auto.evolve": "false",
    "insert.mode": "upsert",
    "delete.enabled": "true",
    "pk.mode": "record_key",
    "behavior.on.null.values": "delete",
    "pk.fields": "name",
    "schema.ignore": "true",
    "type.name": "",
    "transforms": "mongoflatten,ReplaceField,dropPrefix",
    "transforms.dropPrefix.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.dropPrefix.regex": "cdc_(.*)",
    "transforms.dropPrefix.replacement": "moderators",
    "transforms.mongoflatten.type": "io.debezium.connector.mongodb.transforms.ExtractNewDocumentState",
    "transforms.mongoflatten.drop.tombstones": "false",
    "transforms.ReplaceField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
    "transforms.ReplaceField.include": "id,name,full_name",
    "output.format.key": "schema",
    "key.print": "true",
    "output.schema.key": "{\"type\":\"record\",\"name\":\"keySchema\",\"fields\":[{\"name\":\"documentKey._id\",\"type\":\"string\"}]}"
  }
}